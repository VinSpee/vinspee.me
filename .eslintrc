{
  "extends": "airbnb",
  "rules": {
    "react/jsx-filename-extension": 0,
    "import/no-extraneous-dependencies": [
      "error", {
         "devDependencies": true,
         "optionalDependencies": false,
         "peerDependencies": false,
      }
    ],
    "import/resolver": {
      "webpack": {
        "config": {
          "resolve": {
            "extenisons": [".js"],
            "root": "./src"
          }
        }
      }
    }
  }
}
